{
	"appId": "app.vercel.sanble",
	"appName": "Sanble",
	"webDir": "dist",
	"bundledWebRuntime": false,
	"plugins": {
		"SplashScreen": {
			"launchAutoHide": false,
			"splashFullScreen": true,
			"splashImmersive": true,
			"backgroundColor": "#FFFFFF",
			"showSpinner": true,
			"spinnerColor": "#FF7315"
		}
	},
	"server": {
		"url": "http://192.168.0.100:3000",
		"cleartext": true
	}
}
